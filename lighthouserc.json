{
  "ci": {
    "collect": {
      "startServerCommand": "ng serve",
      "numberOfRuns": 1,
      "url": ["https://danielyefet.medium.com/pimp-your-pipeline-with-lighthouse-ci-75801a86a860"],
      "settings": {
        "preset": "desktop",
        "chromeFlags": "--no-sandbox --disable-dev-shm-usage --headless --in-process-gpu",
        "skipAudits": ["uses-http2", "uses-long-cache-ttl", "link-text"]
      }
    },
    "upload": {
      "allowOverwriteOfLatestBranchBuild": true,
      "ignoreIfBuildExists": true,
      "target": "lhci",
      "token": "c4b6c03c-8711-4234-8a61-aee546b0ada0",
      "serverBaseUrl": "http://localhost:9001"
    }
  }
}
